{
    "name": "ellipse/router-fastroute",
    "type": "library",
    "description": "FastRoute adapter for the ellipse router",
    "keywords": [
        "psr-7",
        "psr-15",
        "router",
        "FastRoute"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/router-fastroute",
    "support": {
        "issues": "https://github.com/ellipsephp/router-fastroute/issues",
        "source": "https://github.com/ellipsephp/router-fastroute"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "ellipse/router-adapter": "^1.0",
        "nikic/fast-route": "^1.2"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.0",
        "eloquent/phony-kahlan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Router\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
